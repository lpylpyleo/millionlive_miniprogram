/**app.wxss**/
@import 'weui.wxss';

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.page__title {
    text-align: left;
    font-size: 20px;
    font-weight: 400;
}

.page__desc {
    margin-top: 5px;
    color: #888888;
    text-align: left;
    font-size: 14px;
}

.page__hd {
    padding: 30px;
}
.page__bd {
    padding-bottom: 30px;
}

.line{
  width: 100%;
  height: 1px;
  background-color: #aaa;
  margin: 0;
}

page{
background-color: #f5f5f5;
}

